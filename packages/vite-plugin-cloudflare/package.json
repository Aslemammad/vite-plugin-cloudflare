{
  "name": "vite-plugin-cloudflare",
  "version": "0.3.1",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c --watch src",
    "typecheck": "tsc --noEmit"
  },
  "main": "./dist/vite.js",
  "module": "./dist/vite.js",
  "types": "./dist/vite.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vite.js",
      "node": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    },
    "./shimmed": {
      "import": "./dist/shimmed.js"
    },
    "./process": {
      "import": "./dist/process.js"
    },
    "./inherits": {
      "import": "./dist/inherits.js"
    },
    "./Buffer": {
      "import": "./dist/Buffer.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aslemammad/tinyspy.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aslemammad/vite-plugin-cloudflare/issues"
  },
  "homepage": "https://github.com/aslemammad/vite-plugin-cloudflare#readme",
  "devDependencies": {
    "@types/node": "^18.6.3",
    "@types/node-fetch": "^2.6.2",
    "@types/rollup-plugin-node-builtins": "^2.1.2",
    "@types/rollup-plugin-node-globals": "^1.4.1",
    "esbuild": "^0.17.5",
    "miniflare": "^2.6.0",
    "setimmediate": "^1.0.5",
    "vite": "4.0.4"
  },
  "peerDependencies": {
    "esbuild": "*",
    "miniflare": "*",
    "vite": "*"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "buffer-es6": "^4.9.3",
    "cac": "^6.7.12",
    "fast-glob": "^3.2.11",
    "mlly": "^0.3.16",
    "modern-node-polyfills": "^0.0.9",
    "node-fetch": "^2.6.1",
    "picocolors": "^1.0.0",
    "process-es6": "^0.11.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "stream-http": "^3.2.0",
    "undici": "^5.8.0",
    "xhr2": "^0.2.1"
  }
}
